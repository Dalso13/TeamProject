    <script>
        $('img[usemap]').rwdImageMaps();
        $(function() {
            $('#worldMap area').on('click', function () {
                continentView($(this).attr('alt'))
            });
            $(".continentImg area").on('click',function () {
                smallContinentView($(this).attr('alt'))
            })
            $('area').on('click', function () {
                alert($(this).attr('alt'))
            })
        });
    </script>

    <script>
        function continentView(a) {
            $("#worldMapView").hide();

            switch (a){
                case "북아메리카" : $("#northAmericaSpace").show();
                    break;
                case "남아메리카" : $("#southAmericaSpace").show();
                    break;
                case "아시아" : $("#asiaSpace").show();
                    break;
                case "유럽" : $("#europeSpace").show();
                    break;
                case "오세아니아" : $("#oceaniaSpace").show();
                    break;
                case "아프리카" : $("#africaSpace").show();
                    break;
            }
            // 이것도 hide hidden 처리는 인식을 못해서 할때마다 선언
            $('img[usemap]').rwdImageMaps();
        }

        function smallContinentView(a) {
            $(".continentImg").hide();
            switch (a){
                case "동아시아" : $("#eastAsiaSpace").show();
                    break;
            }
            $('img[usemap]').rwdImageMaps();
        }
    </script>
